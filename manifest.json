{
  "manifest_version": 2,
  "name": "Gitlab Swagger Utilities",
  "version": "1.0.6",
  "description": "Edit and preview swagger files directly in Gitlab",
  "author": "Ilija Tesic <ilija.tesic@gmail.com>",
  "homepage_url": "https://github.com/ITesic/gitlab-swagger-utilities",

  "default_locale": "en",
  "icons": {
    "16": "icons/icon32.png",
    "19": "icons/icon38.png",
    "24": "icons/icon48.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon256.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://gitlab.com/*",
        "https://gitlab.com/*",
        "https://gitlab.samoletgroup.ru/*",
       
      ],
      "js": [
        "src/content/inject.js",
        "src/content/swagger-ui-bundle.js"
      ],
      "css": [
        "src/content/inject.css",
        "src/content/swagger-ui.css"
      ]
    }
  ]
}
